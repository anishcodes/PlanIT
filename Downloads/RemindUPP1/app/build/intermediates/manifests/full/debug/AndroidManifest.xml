<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.remindupp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.PlanIT.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.PlanIT.BunkoMeter"
            android:label="@string/title_activity_bunko_meter" >
        </activity>
        <activity
            android:name="com.example.PlanIT.NewTable"
            android:label="@string/title_activity_new_table" >
        </activity>
        <activity
            android:name="com.example.PlanIT.EditTable"
            android:label="@string/title_activity_edit_table" >
        </activity>
    </application>

</manifest>